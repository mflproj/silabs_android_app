<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="@dimen/adapter_layout_padding"
    android:theme="@style/AppTheme.Light">

    <Button
        android:id="@+id/btn_share_keys"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/network_dialog_share_keys_button" />

    <Button
        android:id="@+id/btn_save_keys"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/network_dialog_save_keys_to_file_button" />

</LinearLayout>
